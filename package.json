{
  "name": "dom_inspector",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.70",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "parcel": "^2.12.0",
    "parcel-bundler": "^1.12.5",
    "pre-commit": "^1.2.2",
    "prettier": "^3.2.5",
    "source-map-explorer": "^2.5.3",
    "typescript": "^4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:widget": "parcel build src/inspector_widget.js src/index.tsx --dist-dir build --no-source-maps --no-cache",
    "build:all": "npm run build && npm run build:widget",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "typescript": "tsc --project tsconfig.json --noEmit",
    "lint": "npm run tsc && npm run eslint && npm run prettier --check \"src/**/*.{ts, tsx}\"",
    "lint:fix": "eslint --cache ./src --ext .jsx,.js,.ts,.tsx --quiet --fix",
    "lint:format": "prettier --write --cache --loglevel warn \"./**/*.{js,jsx,ts,tsx,css,md,json}\" --ignore-path .eslintignore",
    "tsc": "tsc --incremental",
    "lint-tsc": "npm run tsc && npm run lint:format && npm run lint:fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "compilerOptions": {
    "baseUrl": "src"
  },
  "pre-commit": [
    "lint-tsc"
  ]
}
